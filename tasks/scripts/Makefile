# -*- Makefile -*-

####################################################################################################

all: code-review.rcc CodeReviewRessource.py

####################################################################################################

%.rcc : %.qrc
	rcc-qt5 -binary $< -o $@

CodeReviewRessource.py : code-review.qrc
	pyrcc5 -o $@ $<

####################################################################################################

clean:
	rm *.py *.rcc
